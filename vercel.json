{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/public/(.*)",
            "destination": "/public/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/src/main.ts"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}